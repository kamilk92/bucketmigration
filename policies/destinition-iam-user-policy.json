{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Effect": "Allow",
      "Action": [
        "s3:ListBucket",
        "s3:GetObject"
      ],
      "Resource": [
        "arn:aws:s3:::pl.kamil.kurek",
        "arn:aws:s3:::pl.kamil.kurek/*"
      ]
    },
    {
      "Effect": "Allow",
      "Action": [
        "s3:ListBucket",
        "s3:PutObject",
        "s3:PutObjectAcl"
      ],
      "Resource": [
        "arn:aws:s3:::pl.kamil.kurek.migrated",
        "arn:aws:s3:::pl.kamil.kurek.migrated/*"
      ]
    }
  ]
}